@font-face {
    font-family: 'Inconsolata';
    <% if (fontPath.includes('/fonts')) { %>
        src: url('<%=fontPath%>/Inconsolata-Regular.ttf') format('truetype');
    <% } else { %>
        src: url('<%=fontPath%>/src/public/fonts/Inconsolata-Regular.ttf') format('truetype');
    <% } %>
    font-weight: 400;
    font-style: normal;
}
@font-face {
    font-family: 'Inconsolata';
    <% if (fontPath.includes('/fonts')) { %>
        src: url('<%=fontPath%>/Inconsolata-Bold.ttf') format('truetype');
    <% } else { %>
        src: url('<%=fontPath%>/src/public/fonts/Inconsolata-Bold.ttf') format('truetype');
    <% } %>
    font-weight: bold;
    font-style: normal;
}